<div>
  {{! <a href='/'>Home</a> }}
  <a href='#'>My Events</a>
  <a href='/events'>Events list</a>
  <a href='/users/profile'>Profile</a>
  {{! <form action='/auth/logout' method='POST'>
    <button>Logout</button>
  </form> }}
</div>

<h1>Events</h1>

<button class='primary-button'><a href='/events/create'>Add new event</a></button>

<div>
  {{#each event}}
    <div>
      <h3>{{this.name}} </h3>
      {{!-- <p>: {{this.image}}</p> --}}
      <p>Location: {{this.location}}</p>
      <p>Date: {{this.date}} </p>
      <p>Price: {{this.price}} â‚¬</p>
      {{!-- <p>Category: {{this.category}} </p> --}}
      {{!-- <p>Description: {{this.description}} </p> --}}

      <button class='third-button'><a href='/events/details/{{id}}'>Event Details</a></button>
      <br />
      <button class='third-button'><a href='/events/edit/{{id}}'>Edit Events</a></button>
      <br />
      <form action='/events/delete/{{id}}' method='POST'>
        <button class='third-button' type='submit'> Delete Event </button>
      </form>

    </div>
  {{/each}}

</div>